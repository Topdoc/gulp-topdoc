{
  "name": "gulp-topdoc-tasks",
  "version": "0.1.0",
  "description": "A Gulp extension to parse Topdoc data from CSS files and generate styleguides",
  "main": "index.js",
  "scripts": {
    "prepare": "npm run compile",
    "compile": "babel -d ./ ./src/",
    "test": "ava test/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Topdoc/gulp-topdoc-tasks.git"
  },
  "keywords": [
    "gulp",
    "topdoc",
    "styleguide",
    "gulpplugin"
  ],
  "author": "Garth Braithwaite <garthdb@gmail.com> (http://garthdb.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Topdoc/gulp-topdoc-tasks/issues"
  },
  "ava": {
    "files": [
      "test.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "include": [
      "./bin/index.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "homepage": "https://github.com/Topdoc/gulp-topdoc-tasks#readme",
  "dependencies": {
    "plugin-error": "^1.0.1",
    "postcss": "^6.0.21",
    "postcss-topdoc": "^0.1.2",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "codecov": "^3.0.0",
    "eslint": "^4.19.1",
    "event-stream": "^3.3.4",
    "gulp": "^3.9.1",
    "nyc": "^11.7.1",
    "stream": "0.0.2",
    "vinyl": "^2.1.0"
  }
}
